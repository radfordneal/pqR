# ${R_HOME}/bin/Rprof -*- sh -*- for processing Rprof() files

args=
while test -n "${1}"; do
  args="${args}nextArg${1}"
  shift
done

## NB: Apple's ICU needs LC_COLLATE set when R is started.
echo 'tools:::.Rprof()' | R_DEFAULT_PACKAGES=utils LC_COLLATE=C "${R_HOME}/bin/R" --vanilla --no-echo --args ${args}
