#: splines.c:154
msgid "'ord' must be a positive integer"
msgstr "'ord'는 반드시 양의 정수이어야 합니다."

#: splines.c:222
#, c-format
msgid "derivs = %d >= ord = %d, but should be in {0,..,ord-1}"
msgstr "derivs = %d >= ord = %d 이지만, {0,..,ord-1} 내에 있어야 합니다."

#: splines.c:225
#, c-format
msgid "derivs[%d] = %d >= ord = %d, but should be in {0,..,ord-1}"
msgstr "derivs[%d] = %d >= ord = %d 이지만, {0,..,ord-1} 내에 있어야 합니다."
